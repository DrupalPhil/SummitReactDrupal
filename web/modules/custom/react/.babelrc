{
  "presets": [
    [
      "@babel/preset-env",
      {
        targets: {
          esmodules: true,
        },
      }
    ],
    "@babel/preset-react",
    {
      "plugins": [
        "@babel/plugin-proposal-export-default-from",
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        ["@babel/plugin-proposal-class-properties", { "loose" : true }]
      ]
    }
  ],
}
